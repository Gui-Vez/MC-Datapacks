# Teleport the armor stand named "middle" to the midpoint
data modify entity @e[type=armor_stand, name="half", limit=1] Pos[0] set from storage main dist.macro.half.x
data modify entity @e[type=armor_stand, name="half", limit=1] Pos[1] set from storage main dist.macro.half.y
data modify entity @e[type=armor_stand, name="half", limit=1] Pos[2] set from storage main dist.macro.half.z